						</svg>
					</a>
				</div>

				<div class="products-grid">
					{featuredProducts.map((product, i) => {
						const resolvedSlug = product.linkSlug ?? product.slug;
						const productData = getProductBySlug(resolvedSlug);
						const canonicalSlug = productData?.slug ?? resolvedSlug;
						const reviewCount = getReviewCount(canonicalSlug);
						const reviewRating = getReviewRating(canonicalSlug);
						const variants = productData ? getWeightVariants(productData.category) : [];
						const variantMultiplier = variants[0]?.multiplier ?? 1;
						const displayPrice = productData ? Math.round(productData.basePrice * variantMultiplier) : product.price;

						return (
							<a href={`/shop/product/${resolvedSlug}/`} class="product-card" style={`--delay: ${i * 0.1}s`}>
							<div class={`product-badge ${product.type.toLowerCase()}`}>{product.type}</div>
							<div class="product-image">
								{productData?.image ? (
									<img src={productData.image} alt={getProductImageAlt(productData)} width="300" height="300" loading="lazy" />
								) : (
									<svg width="64" height="64" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1">
